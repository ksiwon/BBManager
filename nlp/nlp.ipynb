    "from transformers import pipeline\n",
    "\n",
    "# 사전 학습된 NER 파이프라인 로드\n",
    "ner_pipeline = pipeline(\"ner\", model=\"monologg/koelectra-base-v3-finetuned-ner\")\n",
    "\n",
    "# 텍스트 분석\n",
    "sentence = \"홍창기가 타석에 나왔습니다.\"\n",
    "entities = ner_pipeline(sentence)\n",
    "print(entities)\n"
   ]
